{
    "expo": {
        "name": "Expense Flex",
        "slug": "expense-flex",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/basicIcons/iconSplash1.png",
        "userInterfaceStyle": "dark",
        "backgroundColor": "#A7BAF2",
        "splash": {
            "image": "./assets/splashScreen2.png",
            "resizeMode": "contain",
            "backgroundColor": "#222d4e"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/basicIcons/iconSplash1.png",
                "backgroundColor": "#000"
            },
            "package": "com.piotrko64.expenseflex"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
